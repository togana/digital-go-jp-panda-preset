{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":dependencyDashboard",
    "group:allNonMajor",
    ":prHourlyLimitNone",
    ":separateMultipleMajorReleases",
    ":prHourlyLimitNone",
    ":prConcurrentLimit20",
    ":pinVersions",
    ":labels(renovate)",
    ":timezone(Asia/Tokyo)"
  ],
  "packageRules": [
    {
      "matchFileNames": [
        "examples/**"
      ],
      "matchManagers": [
        "npm"
      ]
    }
  ],
  "rebaseWhen": "never",
  "schedule": [
    "* 8 * * 6"
  ]
}
