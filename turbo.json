{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "prepare": {
      "dependsOn": ["^prepare"],
      "cache": false,
      "outputs": ["dist/**", "styled-system/**"]
    },
    "build": {
      "dependsOn": ["^build"], 
      "outputs": [".next/**", "!.next/cache/**", "dist/**", "styled-system/**"]
    },
    "build:studio": {
      "dependsOn": ["example-studio#build"], 
      "outputs": ["dist/**", "styled-system/**"]
    },
    "ci": {}
  }
}
