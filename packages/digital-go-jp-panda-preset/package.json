{
  "name": "@togana/digital-go-jp-panda-preset",
  "version": "0.0.30",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "pnpm run build",
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "format": "biome check --write --unsafe .",
    "lint": "biome lint .",
    "ci": "biome ci . && pnpm test",
    "prepublishOnly": "pnpm build"
  },
  "author": "togana",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@tsconfig/node20": "20.1.6",
    "@vitest/coverage-v8": "3.2.4",
    "tsup": "8.5.0",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "@digital-go-jp/design-tokens": "1.1.1",
    "@pandacss/types": "1.2.0"
  }
}
