{
  "name": "@togana/digital-go-jp-panda-preset",
  "version": "0.0.34",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "pnpm run build",
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "format": "biome check --write --unsafe .",
    "lint": "biome lint .",
    "ci": "biome ci . && pnpm test",
    "prepublishOnly": "pnpm build"
  },
  "author": "togana",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "2.3.0",
    "@tsconfig/node24": "24.0.3",
    "@vitest/coverage-v8": "4.0.13",
    "tsup": "8.5.0",
    "typescript": "5.9.3",
    "vitest": "4.0.13"
  },
  "dependencies": {
    "@digital-go-jp/design-tokens": "1.1.2",
    "@pandacss/types": "1.4.3"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/togana/digital-go-jp-panda-preset"
  }
}
