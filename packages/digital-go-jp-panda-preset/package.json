{
  "name": "@togana/digital-go-jp-panda-preset",
  "version": "0.0.27",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "pnpm run build",
    "build": "tsup",
    "format": "biome check --write --unsafe .",
    "lint": "biome lint .",
    "ci": "biome ci .",
    "prepublishOnly": "pnpm build"
  },
  "author": "togana",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@tsconfig/node20": "20.1.6",
    "tsup": "8.5.0",
    "typescript": "5.9.2"
  },
  "dependencies": {
    "@digital-go-jp/design-tokens": "1.1.1",
    "@pandacss/types": "0.54.0"
  }
}
